{
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/examples/vanilla/"
    },
    {
      "source": "/examples/",
      "destination": "/examples/vanilla/"
    },
    {
      "source": "/examples/:path((?!vanilla/).*)",
      "destination": "/examples/vanilla/:path*"
    }
  ]
}
