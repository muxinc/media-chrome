{
  "name": "media-chrome",
  "version": "0.0.11",
  "description": "Custom elements (web components) for making audio and video player controls that look great in your website or app.",
  "type": "module",
  "main": "dist/media-chrome.js",
  "files": [
    "./dist/*",
    "README.md",
    "./examples/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "dev": "npm run clean && rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish-patch": "yarn run build && np patch --no-tests --any-branch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muxinc/media-chrome.git"
  },
  "keywords": [
    "webcomponent",
    "html5",
    "video",
    "audio",
    "media",
    "player",
    "controls"
  ],
  "author": "@muxinc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muxinc/media-chrome/issues"
  },
  "homepage": "https://github.com/muxinc/media-chrome#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-livereload": "^4.0.2",
    "rollup": "^2.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.2",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
